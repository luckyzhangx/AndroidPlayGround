<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RelativeFrameLayout">
        <attr name="margin_vertical" format="dimension" />
        <attr name="margin_horizontal" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RatioFrameLayout">
        <attr name="measureBase" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="ratioHw" format="float" />
    </declare-styleable>

    <declare-styleable name="AndRatingBar">
        <!-- color of star -->
        <attr format="color|reference" name="starColor" />

        <!-- color of secondary star -->
        <attr format="color|reference" name="subStarColor" />

        <!-- color of background star -->
        <attr format="color|reference" name="bgColor" />

        <!-- drawable of star -->
        <attr format="reference" name="starDrawable" />

        <!-- drawable of background star -->
        <attr format="reference" name="bgDrawable" />

        <!-- if keep the origin color of star drawable -->
        <attr format="boolean" name="keepOriginColor" />

        <!-- the scale factor of ratingbar that can change the spacing of star -->
        <attr format="float" name="scaleFactor" />

        <!-- additional the spacing of the star -->
        <attr format="dimension" name="starSpacing" />

        <!-- right to left -->
        <attr format="boolean" name="right2Left" />
    </declare-styleable>
</resources>